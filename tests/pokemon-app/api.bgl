
from 'https://raw.githubusercontent.com/brundonsmith/bagel/master/lib/bgl/http.bgl' import { get }

export type Card = { imageUrl: string }

export type CardsResponse = {
    cards: Card[]
}

export func searchCards(str: string): Plan<CardsResponse> =>
    const res = await get('https://api.pokemontcg.io/v1/cards?name=${str}'),
    res
