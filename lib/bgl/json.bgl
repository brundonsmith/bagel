export js func parseJson(str: string): JSON => {# return JSON.parse(str) #}

export js func stringifyJson(val: JSON, indentation: number): string => {#
    return JSON.stringify(val, x => typeof x === 'undefined' ? null : x, indentation)
#}

export type JSON =
    | {[string]: JSON}
    | JSON[]
    | string
    | number
    | boolean
    | nil